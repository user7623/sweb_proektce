@model IEnumerable<SWEB_app.Models.Teacher>

@{
    ViewData["Title"] = "addPicture";
}

<h1>addPicture</h1>
<br />
<br />
<form method="post" asp-controller="Teachers" asp-action="showPicture" enctype="multipart/form-data">
    <input type="file" name="pic" /><br />
    <label>Enter first name of teacher</label>
    <input type="text" name="firstName" /><br />
    <label>Enter last name of teacher</label>
    <input type="text" name="lastName" /><br />
    <input type="submit" value="upload" />
</form>
